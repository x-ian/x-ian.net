<body>
<!-- wrap START -->
<div id="wrap">

<!-- container START -->
<div id="container">

<!-- header START -->
<div id="header">

	<div id="caption">
		<h1 id="title"><a href="/">Simple thoughts</a></h1>
		<div id="tagline">#{programming} || #{mountains}</div>
	</div>

	<div class="fixed"></div>
</div>
<!-- header END -->

<!-- content START -->
<div id="content">

	<!-- main START -->
	<div id="main">

	<div id="postpath">
		<a title="Go to homepage" href="/">Home</a>
		 &gt; <a href="/category/{{ pagination.items[0].data.tags[0] }}/" title="View all posts in {{ pagination.items[0].data.tags[0] }}" rel="category tag">{{ pagination.items[0].data.tags[0] }}</a> &gt; {{ pagination.items[0].data.title }}	</div>

		{{ content | safe }} 

	<div class="messagebox">
		Comments are closed.
	</div>

	<div id="postnavi">		
		<span class="prev">{% if pagination.href.previous %}<a href="{{ pagination.href.previous }}" rel="next">Previous</a>{% else %}Previous{% endif %}</span>
		
		<span class="next">{% if pagination.href.next %}<a href="{{ pagination.href.next }}" rel="prev">Next</a>{% else %}Next{% endif %}</span>

		
		<div class="fixed"></div>
	</div>

</div>
	<!-- main END -->

<!-- sidebar START -->
<div id="sidebar">

<!-- sidebar north START -->
<div id="northsidebar" class="sidebar">

	<!-- feeds -->
	<div class="widget widget_feeds">
		<div class="content">
			<div id="subscribe"><a rel="external nofollow" id="feedrss" title="Subscribe to this blog..." href="http://x-ian.net/feed/"><abbr title="Really Simple Syndication">RSS</abbr></a></div>
						<div class="fixed"></div>
		</div>
	</div>

	<!-- showcase -->
	
<div id="pages-2" class="widget widget_pages"><h3>Pages</h3>		<ul>
